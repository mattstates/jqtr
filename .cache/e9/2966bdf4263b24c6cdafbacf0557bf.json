d":{"line":100,"column":4},"source":"Components/SearchBar.jsx","original":{"line":65,"column":108}},{"generated":{"line":100,"column":16},"source":"Components/SearchBar.jsx","original":{"line":65,"column":120}},{"name":"searchString","generated":{"line":100,"column":18},"source":"Components/SearchBar.jsx","original":{"line":65,"column":122}},{"generated":{"line":101,"column":0},"source":"Components/SearchBar.jsx","original":{"line":65,"column":56}},{"generated":{"line":101,"column":4},"source":"Components/SearchBar.jsx","original":{"line":56,"column":8}},{"generated":{"line":101,"column":5},"source":"Components/SearchBar.jsx","original":{"line":55,"column":4}},{"generated":{"line":102,"column":0},"source":"Components/SearchBar.jsx","original":{"line":68,"column":1}}],"sources":{"Components/SearchBar.jsx":"import './SearchBar.scss';\nimport React, { useState, useEffect, useContext, useRef } from 'react';\nimport SearchDropDown from './SearchDropDown.jsx';\nimport TimeInStatusContext from '../contexts/TimeInStatusContext.js';\nimport { getFetchOptions } from '../utils/apiUtils.js';\nimport { jiraApiUrlAutocomplete } from '../utils/urls.js';\nimport { storageAvailable } from '../utils/utils.js';\n\nfunction getAutoCompleteData(updateState, options) {\n    fetch(jiraApiUrlAutocomplete, getFetchOptions(options))\n        .then((res) => {\n            return res.json();\n        })\n        .then((json) => {\n            updateState(json);\n        })\n        .catch((err) => console.log(err, err.message));\n}\n\nexport default function SearchBar({ initialSearchString, submitCallback, searchAbortController }) {\n    const searchRef = useRef();\n    const [searchString, updateSearchString] = useState('');\n    const [autoCompleteData, updateAutocompleteData] = useState(null);\n\n    // TODO: Consider removing the timeInStatusContext reset to a different component. Having the\n    // App component handle the context was causing tons of re-renders of the whole app due to context\n    // data changes...\n    const timeInStatusContext = useContext(TimeInStatusContext);\n\n    useEffect(() => {\n        searchRef.current.value = initialSearchString;\n    }, [initialSearchString]);\n\n    useEffect(() => {\n        const abortController = new AbortController();\n        const signal = abortController.signal;\n        getAutoCompleteData(updateAutocompleteData, { signal });\n        return () => {\n            abortController.abort();\n        };\n    }, []);\n\n    function submit(e) {\n        if (e.which !== 13 || searchRef.current.value.trim().length < 1) {\n            return;\n        }\n        if (storageAvailable) {\n            window.localStorage.lpTimeRemainingQuery = searchRef.current.value;\n        }\n\n        submitCallback(searchRef.current.value, searchAbortController);\n        timeInStatusContext.clearTimeInStatus();\n    }\n\n    return (\n        <React.Fragment>\n            <input\n                type=\"text\"\n                ref={searchRef}\n                onChange={() => {\n                    updateSearchString(searchRef.current.value);\n                }}\n                onKeyUp={submit}\n            />\n            {autoCompleteData && searchString !== '' && <SearchDropDown autoCompleteData={autoCompleteData} searchString={searchString} />}\n        </React.Fragment>\n    );\n}\n"},"lineCount":null}},"error":null,"hash":"95c2d287d90d0c59310ce1c40d91c329","cacheData":{"env":{}}}